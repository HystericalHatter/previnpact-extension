        if (window.innerWidth < 900) {
            aside.classList.remove("fixed-aside");
        }
        if (window.innerWidth > 900 && window.innerWidth < 1400) {
            if (positionClientY < 0) {
                aside.style.right = max_right.getBoundingClientRect().left + "px";
            }
        }
        if (window.innerWidth > 1400) {
            aside.style.right = max_right.getBoundingClientRect().left + "px";
        }
    };

    window.addEventListener('scroll', function() {
        let positionClientY = beging_fixed.getBoundingClientRect().top - 90;
        if (window.innerWidth > 900) {
            if (positionClientY < 0) {
                aside.classList.add("fixed-aside");
                aside.style.right = max_right.getBoundingClientRect().left + "px";
            } else {
                aside.classList.remove("fixed-aside");
            }
        }
