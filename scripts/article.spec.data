<script defer="">
    function toggleDropdown(event) {
        event.children[1].classList.toggle("dropdown-content-show");
    }

    window.addEventListener('click', function(event) {
        const dropdownButtonElement = Array.from(document.getElementsByClassName('dropdown-button'));
        const dropdownContent = Array.from(document.getElementsByClassName("dropdown-content"));
        if (dropdownButtonElement[0]) {
            if ((!dropdownButtonElement[0].contains(event.target))) {
                dropdownContent[0].classList.remove("dropdown-content-show");
            }
        }
        if (dropdownButtonElement[1]) {
            if ((!dropdownButtonElement[1].contains(event.target))) {
                dropdownContent[1].classList.remove("dropdown-content-show");
            }
        }
        if (dropdownButtonElement[2]) {
            if ((!dropdownButtonElement[2].contains(event.target))) {
                dropdownContent[2].classList.remove("dropdown-content-show");
            }
        }
    });
 
    function copyLink() {
        var link = window.location.href;
        var tempInput = document.createElement("input");
        tempInput.style = "position: absolute; left: -1000px; top: -1000px";
        tempInput.value = link;
        document.body.appendChild(tempInput);
        tempInput.select();
        document.execCommand("copy");
        document.body.removeChild(tempInput);
    }

    function shareOnTwitter() {
        var url = window.location.href;
        var title = document.querySelector('h1').textContent;
        var tweetUrl = "https://twitter.com/intent/tweet?text=" + encodeURIComponent(title) + "&url=" + encodeURIComponent(url) + "&related=related_twitter_handles" /* &image=" + encodeURIComponent(image) */ ;
        window.open(tweetUrl, '_blank');
    }

    function shareOnFacebook() {
        var url = window.location.href;
        var title = document.querySelector('h1').textContent;
        var facebookUrl = "https://www.facebook.com/sharer/sharer.php?u=" + encodeURIComponent(url);
        window.open(facebookUrl, '_blank');
    }

    function shareOnMastodon() {
        var url = window.location.href;
        var title = document.querySelector('h1').textContent;
        // var mastodonUrl = "https://mastodon.social/sharer?text="  + encodeURIComponent(title) + "&url" + encodeURIComponent(url);
        // window.open(mastodonUrl, '_blank');

        const mastodonInstance = prompt(`Partage: "${url}"\n Merci de saisir votre instance de Mastodon (e.g. mastodon.social) pour partager`);
    	if (mastodonInstance == null) {
	    	return;
	    }
	    if (mastodonInstance.indexOf('/') === -1) {
		    window.open('https://' + mastodonInstance + '/share?text=' + encodeURIComponent(title) + ' ' + encodeURIComponent(url), "_blank");
	    } else {
		    alert("Merci de saisir votre instance sans le https:// ou tout autre chemin");
	    }
    }

    function shareOnLinkedin() {
        var url = window.location.href;
        var title = document.querySelector('h1').textContent;
        var linkedInUrl = "https://www.linkedin.com/sharing/share-offsite/?url=" + encodeURIComponent(url) + "&title=" + encodeURIComponent(title);
        window.open(linkedInUrl, '_blank');
    }
</script>
