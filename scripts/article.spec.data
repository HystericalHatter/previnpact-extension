    window.addEventListener('scroll', function(event) {
        const shareBottom = document.getElementById('share-bottom');
        const shareMobile = document.getElementById('share-mobile');
        if (shareMobile) {
            if (window.innerHeight - shareBottom.getBoundingClientRect().bottom > 0) {
                shareMobile.classList.add('share-mobile-hidden');
            } else {
                shareMobile.classList.remove('share-mobile-hidden');
            }
        }
    })
</script>


